(println "Enter operator (one of + - * /) and your two operands:")
(set op (read))
(set n1 (num (read)))
(set n2 (num (read)))
(if (= op "+")
    (println n1 '+ n2 '= (+ n1 n2))
  (= op "-")
    (println n1 '- n2 '= (- n1 n2))
  (= op "*")
    (println n1 '* n2 '= (* n1 n2))
  (= op "/")
    (println n1 '/ n2 '= (/ n1 n2))
  (println "Unknown operator!"))
